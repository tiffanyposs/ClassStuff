<html>
  <head>
    <title></title>
  </head>
  <body>

    <% @articles.each do |article| %>
      <%= link_to article.headline, article_path(article) %>
      <br>
      <%= link_to article.author.name, author_path(article.author) %>
      <br>
      <%= article.content %>
      <br>
      <%= image_tag article.photo_url, height:300 %>
      <br>
    <% end %>

  </body>
</html>
