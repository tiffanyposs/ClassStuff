<%= form_for @article, url: {action: 'update'} do |f|%>
  <%= f.label :headline%>
  <%= f.text_field :headline, value: @article.headline %>
  <br>
  <%= f.label :content%>
  <%= f.text_area :content, value: @article.content %>
  <br>
  <%= f.label :photo_url%>
  <%= f.text_field :photo_url, value: @article.photo_url %>
  <br>
  <%= f.submit %>
<% end %>
